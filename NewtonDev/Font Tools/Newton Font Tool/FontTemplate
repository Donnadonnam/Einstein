constant kUnicode := 0;constant kFix1 := 0x00010000;refNum := OpenResFileX(HOME & "font_output");gSFNTData1 := GetNamedResource("sfnt", "font_output", 'sfnt);CloseResFileX(refNum);output := {|font_output|: {	name:		"font_output",	encoding:	kUnicode,	macFontID:	1025,	screenSym:	'|font_output|,	psName:		'Helvetica,	psScale:	kFix1,		// no scaling	plainData:		gSFNTData1,	boldData:		gSFNTData1,	italicData:		nil,	boldItalicData:	nil,}};