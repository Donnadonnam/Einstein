If "{{NCT_Cur_Rex-dir}}" == "" 	Set saved_Dir "`Directory -q`"	Directory {{NCTSources}}	Set _request 0	if "{{NCT_Cur_Rex-path}}" == ""	Set NCT_Cur_Rex-path "¶"`GetFileName -q  -t 'TEXT' -b "Set To" -m "Select a Rex config file:" && Set _request {status} || Set Status 0`¶""	End		if !_request 		if "`Exists {NCTTools}Rex`"			{NCTTools}Rex  "{{NCT_Cur_Rex-path}}"		end	End	directory "{saved_Dir}"	unset _request	unset saved_DirElse If "{{NCT_Cur_Rex-path}}" ­ ""# What should this do??# look for a TEXT file with .config suffix?# what if there is more than one?# Should i care about it at all now?# This is licensee support so cut and run with something	if "`Exists {NCTTools}Rex`"		{NCTTools}Rex  "{{NCT_Cur_Rex-path}}"	endElseEnd