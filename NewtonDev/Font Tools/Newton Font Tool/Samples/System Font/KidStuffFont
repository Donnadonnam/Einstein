constant kUnicode := 0;constant kFix1 := 0x00010000;refNum := OpenResFileX(HOME & "KidStuff-Plain");gSFNTData1 := GetNamedResource("sfnt", "KidStuff-Plain", 'sfnt);CloseResFileX(refNum);output := {|KidStuff-Plain|: {	name:		"KidStuff-Plain",	encoding:	kUnicode,	macFontID:	1025,	screenSym:	'|KidStuff-Plain|,	psName:		'Helvetica,	psScale:	kFix1,		// no scaling	plainData:		gSFNTData1,	boldData:		gSFNTData1,	italicData:		nil,	boldItalicData:	nil,}};