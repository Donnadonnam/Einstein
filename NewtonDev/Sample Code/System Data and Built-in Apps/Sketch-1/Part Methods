nt format and the frame format	UnregisterViewDef( kPrintFormatSymbol, kDefSymbol );	UnregisterViewDef( kFrameFormatSymbol, kDefSymbol );		// Unregister the sample tool for Sketch.  First make sure	// that Newton Works is installed on the Newton device.//	if GetRoot().newtWorks then//		GetRoot().newtWorks:UnregNewtWorksTool( kSampleToolSymbol );end;