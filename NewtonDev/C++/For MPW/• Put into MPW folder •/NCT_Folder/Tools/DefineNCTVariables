et NCT-link-options ' -dsuppress -remove -aif -bin ' ; Export NCT-link-options### Lib ###Set NCT-lib ARMLink ; Export NCT-libSet NCT-lib-Options ' -dsuppress -aof' ; Export NCT-lib-Options### Objectsout ###Set NCT-ObjectsDebug "Objects" ; Export NCT-ObjectsDebugSet NCT-ObjectsNoDebug "ObjectsNoDebug" ; Export NCT-ObjectsNodebugSet NCTBuildDebugItem "Build with Debug" ;export NCTBuildDebugItemSet NCTBuildNoDebugItem "Build with NoDebug" ;export NCTBuildNoDebugItemSet NCT-forDebug "-d forDebug"  ; Export NCT-forDebug# default to Debug mode buildSet NCT_DebugSwitch "{NCT-forDebug}" ; export NCT_DebugSwitch  Set NCT-ObjectOut "{NCT-ObjectsDebug}" ; Export NCT-ObjectOutSet NCT-setfile-cmd SetFileExport NCT-Setfile-cmdSet NCT-packer PackerExport NCT-packerSet NCT-AIFtoNTK AIFtoNTKExport NCT-AIFtoNTKSet NCT-Cmodule-filetype '"ntkc"'Export NCT-Cmodule-filetypeSet NCT-Cmodule-filecreator '"NTP1"'Export NCT-Cmodule-filecreatorSet NCT-pram-options ""Export NCT-Pram-optionsIf "{NCT_DoesNotExist}" == ""	Set NCT_DoesNotExist 'Beep 2c,7'EndExport NCT_DoesNotExistIf "{NCTBuildDiagnosticsCmdKey}" == ""	Set NCTBuildDiagnosticsCmdKey '/I'EndIf "{NCTLaunchImageCmdKey}" == ""	Set NCTLaunchImageCmdKey '/L'EndSet NCT_ROMImages-dir ""Export NCT_ROMImages-dirSet NCT_Cur_Image-path ""Export NCT_Cur_Image-pathSet NCT_Debugger_name "Newtsbug"    # this can be overridden in the NCTBuild¥UserPrefs scriptExport NCT_Debugger_nameif "`Exists "{NCTTools}DefineDDKVariables" `" ­ ""	Execute "{NCTTools}DefineDDKVariables"  	# define all the DDK- shell variables used in old DDK Makefilesendalias stf "SearchTextFiles"