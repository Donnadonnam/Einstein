Set saved_Dir "`Directory -q`"Directory "{Boot}"Set _NCT_backup_path "`GetFileName -b "From here" -wd -q -m "Locate ÔMPW AdditionsÕ folder" || Set Status 0`"if  "{_NCT_backup_path}" == ""	directory "{saved_Dir}"	Exit 0endEcho  > "{NCTScratch}Backuplist"Flush # clear stuff out before backup upIF "{NCTBuildResultsFile}"	Open -n "{NCTBuildResultsFile}"	SaveOnClose -n "{NCTBuildResultsFile}"	Set NCTbkup-redir "³³ ¶"{NCTBuildResultsFile}¶""ELSE If "{NCTSendSearchResultsToWorksheet}"	Set NCTbkup-redir "³³ ¶"{Worksheet}¶""ELSE	Set NCTbkup-redir ""END(if "`Exists -d "{_NCT_backup_path}NCT_Folder:Tools:"`" ­ ""	Backup  -p -a -from "{_NCT_backup_path}NCT_Folder:Tools:" -to "{NCTTools}" || Set Status 0endif "`Exists -f "{_NCT_backup_path}UserStartup¥NewtonC++Tools"`" ­ ""	Backup -p "{_NCT_backup_path}UserStartup¥NewtonC++Tools" -to "{MPW}" || Set Status 0endif "`Exists -f "{_NCT_backup_path}NCTBuild¥UserPrefs"`" ­ ""	Backup -p "{_NCT_backup_path}NCTBuild¥UserPrefs" -to "{MPW}" || Set Status 0endif "`Exists -d "{_NCT_backup_path}Includes:"`" ­ ""	Backup -p -a -n -c -r -from "{_NCT_backup_path}Includes:" -to "{NCT_Includes}" || Set Status 0endif "`Exists -d "{_NCT_backup_path}Libraries:"`" ­ ""	Backup  -a -from "{_NCT_backup_path}Libraries:" -to "{NCT_Libraries}" || Set Status 0endif "{DDK_Includes-dir}" ­ ""	if "`Exists -d "{_NCT_backup_path}DDKIncludes:"`" ­ ""		Backup -p -a -n -c -r -from "{_NCT_backup_path}DDKIncludes:" -to "{DDK_Includes-dir}" || Set Status 0	endendif "{DDK_Libraries-dir}" ­ ""	if "`Exists -d "{_NCT_backup_path}DDKLibraries:"`" ­ ""		Backup -p -a -from "{_NCT_backup_path}DDKLibraries:" -to "{DDK_Libraries-dir}" || Set Status 0	endend) >> "{NCTScratch}Backuplist" {NCTbkup-redir}	Flush	Doit "{NCTScratch}Backuplist"			directory "{saved_Dir}"Set nct-restart-mpw 0(Search -i -sf /'UserStartup¥NewtonC'/ "{NCTScratch}Backuplist" && Set nct-restart-mpw 1) > Dev:Nullif Â{nct-restart-mpw}	(Search -i -sf /'NCTBuild¥UserPrefs'/ "{NCTScratch}Backuplist" && Set nct-restart-mpw 1) > Dev:Nullendif Â{nct-restart-mpw}(Search -i -sf /'NCTSearchMenuInstall'/ "{NCTScratch}Backuplist" && Set nct-restart-mpw 1) > Dev:Nullendif Â{nct-restart-mpw}(Search -i -sf /'NCTProjectMenuAddons'/ "{NCTScratch}Backuplist" && Set nct-restart-mpw 1) > Dev:Nullend# re-initialize NCT system in case a bug fix is in a startup script# this won't execute a change to UserStartup¥NewtonC++Tools if {nct-restart-mpw}	Alert "Some of the scripts only called during the MPW startup sequence have been updated.¶n¶nPlease Quit and restart MPW to update your ¶nNewton C++ Tools build system."	Exit 0else	Execute "{NCTTools}ActivateNCT" ·· "{WorkSheet}"end